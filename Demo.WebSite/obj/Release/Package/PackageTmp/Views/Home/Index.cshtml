@model List<Demo.Site.Models.LoginLogModel>
@{
    ViewBag.Title = "主页";
}
@section head{
    <script src="../../Scripts/js/Index.js"></script>
}
<ul class="dropdown-menu" id="contextMenu">
        <li>
            <a href="javascript:alert('添加');">添加</a>    
        </li>
        <li>
            <a href="javascript:alert('删除');">删除</a>    
        </li>
        <li>
            <a href="javascript:alert('关于我们');">关于我们</a>    
        </li>
</ul>
<table id="demo1" class="table table-hover">
    <tbody>
        @if (Model != null && Model.Any())
        {
            foreach (var item in Model)
            {
            <tr>
                <td>@item.Id</td>
                <td>@item.IpAddress</td>
                <td>@item.CreateTime.ToString("yyyy-MM-dd").</td>
            </tr>
            }
        }
    </tbody>
    <tfoot>
        <tr>
            <td colspan="3">@Html.Raw(ViewBag.Pager)</td>
        </tr>
    </tfoot>
</table>
  
<a class="btn btn-default btn-round" href="#myModal1" data-toggle="modal">测试1</a>
<a class="btn btn-success btn-round" href="#myModal2" data-toggle="modal">测试2</a>

<div class="modal fade" id="myModal1" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
                <h4 class="modal-title">确认删除</h4>
            </div>
            <div class="modal-body">
                您确定要cddd删除？删除后将不可恢复
            </div>
            <div class="modal-footer">
                <button data-dismiss="modal" class="btn btn-default" type="button">关闭</button>
                <button data-dismiss="modal" class="btn btn-warning" type="button" onclick="" >确定</button>
            </div>
        </div>
    </div>
</div>

<div class="modal fade" id="myModal2" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
